{
  "name": "egor.philippov.ca",
  "version": "1.0.0",
  "sideEffects": false,
  "license": "UNLICENSED",
  "scripts": {
    "analyze": "cross-env ANALYZE=true pnpm build",
    "build": "next build",
    "compile": "tsc --noEmit",
    "deploy:gh": "next build && touch out/.nojekyll && cd out && git init && git add -A && git commit -m 'Deploy' && git push -f git@github.com:lone-cloud/egorphilippov.git master:gh-pages",
    "dev": "next --turbopack",
    "lint": "eslint .",
    "prepare": "husky",
    "start": "npx serve@latest out",
    "pretty": "prettier --write \"**/*.{js,jsx,ts,tsx,md,yml,css,json}\""
  },
  "dependencies": {
    "next": "^15.4.5",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@next/bundle-analyzer": "^15.4.5",
    "@next/eslint-plugin-next": "^15.4.5",
    "@types/node": "^24.2.0",
    "@types/react": "19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-sonarjs": "^3.0.4",
    "gh-pages": "^6.3.0",
    "husky": "9.1.7",
    "lint-staged": "^16.1.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,css}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ]
}
